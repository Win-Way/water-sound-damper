   "source": [
    "# Plot optimization results\n",
    "fig, ((ax1, ax2), (ax3, ax4)) = plt.subplots(2, 2, figsize=(15, 10))\n",
    "\n",
    "# Ball radius vs energy absorption\n",
    "ball_radii = np.linspace(0.001, 0.01, 20)  # 1mm to 10mm\n",
    "energy_absorption = np.exp(-ball_radii*1000/3) * (ball_radii*1000)**0.5  # Simplified model\n",
    "\n",
    "ax1.plot(ball_radii*1000, energy_absorption, 'b-', linewidth=2)\n",
    "ax1.axvline(x=2.9, color='red', linestyle='--', alpha=0.7, label='Optimal (2.9mm)')\n",
    "ax1.set_xlabel('Ball Radius (mm)')\n",
    "ax1.set_ylabel('Energy Absorption')\n",
    "ax1.set_title('Energy Absorption vs Ball Size')\n",
    "ax1.grid(True, alpha=0.3)\n",
    "ax1.legend()\n",
    "\n",
    "# Optical scattering efficiency\n",
    "optical_amplification = np.pi * ball_radii**2 / ball_radii\n",
    "ax2.plot(ball_radii*1000, optical_amplification*1e6, 'g-', linewidth=2)\n",
    "ax2.axvline(x=10.0, color='orange', linestyle='--', alpha=0.7, label='Optimal Optical (10mm)')\n",
    "ax2.set_xlabel('Ball Radius (mm)')\n",
    "ax2.set_ylabel('Optical Amplification (√ó10‚Å∂)')\n",
    "ax2.set_title('Optical Signal Strength vs Ball Size')\n",
    "ax2.grid(True, alpha=0.3)\n",
    "ax2.legend()\n",
    "\n",
    "# Temperature effects on buoyancy\n",
    "temps = np.linspace(25, 70, 50)\n",
    "buoyancy_factor = 1 + temps * 0.0008  # Thermal expansion\n",
    "ax3.plot(temps, buoyancy_factor, 'purple', linewidth=2)\n",
    "ax3.axhline(y=1.0, color='black', linestyle=':', alpha=0.5)\n",
    "ax3.set_xlabel('Temperature (¬∞C)')\n",
    "ax3.set_ylabel('Buoyancy Factor')\n",
    "ax3.set_title('Thermal Buoyancy Effects')\n",
    "ax3.grid(True, alpha=0.3)\n",
    "\n",
    "# Sound wave pressure field\n",
    "x = np.linspace(0, 100, 100)\n",
    "y = np.linspace(0, 50, 50)\n",
    "X, Y = np.meshgrid(x, y)\n",
    "pressure = np.sin(2 * np.pi * X / 50) * np.cos(np.pi * Y / 25)  # Standing wave\n",
    "im = ax4.contourf(X, Y, pressure, levels=20, cmap='RdBu')\n",
    "ax4.set_xlabel('Horizontal Position (mm)')\n",
    "ax4.set_ylabel('Vertical Position (mm)')\n",
    "ax4.set_title('Sound Wave Pressure Field (Standing Wave)')\n",
    "plt.colorbar(im, ax=ax4, label='Pressure (relative)')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('wax_ball_analysis.png', dpi=300, bbox_inches='tight')\n",
    "plt.show()"
   ]
